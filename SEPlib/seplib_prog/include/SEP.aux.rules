CLIB_DISTR=
CLIB_LOCAL=
FCLIB_LOCAL=
FCLIB_DISTR=
EXTRA_LIBRARIES=${CLIB_DISTR} ${CLIB_LOCAL}  ${FCLIB_DISTR} ${FCLIB_LOCAL}


CPROG_DISTR=
CPROG_LOCAL=
FCPROG_DISTR=
FCPROG_LOCAL=
EXTRA_PROGRAMS=${CPROG_DISTR} ${CPROG_LOCAL}  ${FCPROG_DISTR} ${FCPROG_LOCAL}

SCRIPTS_LOCAL=
SCRIPTS_DISTR=
bin_SCRIPTS=${SCRIPTS_LOCAL} ${SCRIPTS_DISTR}

CDIR_DISTR=.

RATSEPME = $(SEPRAT) 

%.f90:  ${srcdir}/%.f90s
	${RATSEPME} -dofpp ${FCDEFINES} ${srcdir}/$*.f90s

%.cpp:	${srcdir}/%.cppsep
	${SEPDOCIT} -C < $< >$@
                                                                                                   
%.c:	${srcdir}/%.csep
	${SEPDOCIT} -C < $< >$@
                                                                                                   
${srcdir}/%.f90s: ${srcdir}/%.r90
	$(RATSEPME) -c -dof90 $<
                                                                                                   
${srcdir}/%.f90s:  ${srcdir}/%.F90sep
	$(RATSEPME)  -dof90  $<

${srcdir}/%.f90s:  ${srcdir}/%.rs90
	$(RATSEPME) -c -dof90 -sepmain $<

AUTOMAKE_OPTIONS=foreign no-dependencies dist-tarZ 1.3
